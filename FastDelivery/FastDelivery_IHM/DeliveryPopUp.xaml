<UserControl
    x:Class="FastDelivery_IHM.DeliveryPopUp"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:FastDelivery_IHM"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300"
    d:DesignWidth="400">

    <Grid Margin="0,0,-188,0">
        <TextBox x:Name="id" HorizontalAlignment="Left" Margin="65,64,0,0" TextWrapping="Wrap" VerticalAlignment="Top" PlaceholderText="ID"/>
        <TextBox x:Name="idPoint" HorizontalAlignment="Left" Margin="64,110,0,0" TextWrapping="Wrap" VerticalAlignment="Top" PlaceholderText="ID Point"/>
        <TextBox x:Name="duree" HorizontalAlignment="Left" Margin="63,157,0,0" TextWrapping="Wrap" VerticalAlignment="Top" PlaceholderText="Durée"/>
        <TimePicker HorizontalAlignment="Left" Margin="306,47,0,0" VerticalAlignment="Top" Header="Début plage"/>
        <TimePicker HorizontalAlignment="Left" Margin="308,129,0,0" VerticalAlignment="Top" Header="Fin plage"/>



    </Grid>
</UserControl>
